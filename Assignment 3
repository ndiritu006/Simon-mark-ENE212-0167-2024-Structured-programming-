#include <stdio.h>

int main() {
    char str[100];
    int i, count = 0;

    printf("Enter a string (no spaces): ");
    scanf("%s", str);   
    for (i = 0; str[i] != '\0'; i++) {
        count++;
    }

    printf("You entered: %s\n", str);
    printf("Number of characters: %d\n", count);

    return 0;
}
