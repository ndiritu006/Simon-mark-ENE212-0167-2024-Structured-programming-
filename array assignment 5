#include <stdio.h>

int main() {
    int n, i, j, temp;
    printf("Input the size of array : ");
    scanf("%d", &n);
    int arr[n];

    for(i = 0; i < n; i++) {
        printf("element - %d : ", i);
        scanf("%d", &arr[i]);
    }

    for(i = 0; i < n; i++) {
        for(j = i + 1; j < n; j++) {
            if(arr[i] > arr[j]) {
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }

    printf("Elements of array in sorted ascending order: ");
    for(i = 0; i < n; i++) printf("%d ", arr[i]);
    return 0;
}
